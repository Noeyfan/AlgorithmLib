#ifndef _NOEY_ALGOS
#define _NOEY_ALGOS

#include "noey_heap.h"
#include "noey_mst.h"
#include "noey_lru.h"

namespace noey
{
  template <typename _Tp, typename _Comp = std::greater<int>>
  using heap = __heap<_Tp, _Comp>;

  using mst = __mst;

  using lru_cache = __lru<int>;
}

#endif //_NOEY_ALGOS
