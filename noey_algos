#ifndef _NOEY_ALGOS
#define _NOEY_ALGOS

#include "noey_heap.h"
#include "noey_mst.h"
#include "noey_lru.h"
#include "noey_skip_list.h"

namespace noey
{
  template <typename _Tp, typename _Comp = std::greater<int>>
  using heap = __heap<_Tp, _Comp>;

  using mst = __mst;

  template <typename _Tp>
  using lru_cache = __lru<_Tp>;

  template <typename _Tp>
  using skip_list = __skip_list<_Tp>;
}

#endif //_NOEY_ALGOS
